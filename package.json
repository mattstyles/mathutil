{
  "name": "mathutil",
  "version": "0.2.0",
  "description": "Various and random collection of maths utilities",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build",
    "prepack": "npm run build",
    "watch": "babel src --out-dir lib --watch",
    "prebuild": "rimraf lib && mkdirp lib",
    "build": "babel src --out-dir lib",
    "preexample": "npm run build",
    "example": "budo examples/turn.js:bundle.js -v --dir examples --live"
  },
  "babel": {
    "presets": [ "urban" ]
  },
  "browserify": {
    "transform": "babelify"
  },
  "keywords": [
    "Math",
    "mathematics",
    "vector",
    "underscore.math",
    "helper",
    "util"
  ],
  "author": "Matt Styles",
  "repository": "git://github.com/mattstyles/mathutil.git",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-urban": "^0.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "budo": "^8.2.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "tape": "^4.5.1"
  }
}
