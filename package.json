{
  "name": "mathutil",
  "version": "2.2.1",
  "description": "Various and random collection of maths utilities",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "jsnext:main": "lib/index.mjs",
  "scripts": {
    "test": "tape -r buba/register spec/*.js",
    "prepublish": "npm run build",
    "prepack": "npm run build",
    "watch": "rollup -c -w",
    "prebuild": "rimraf lib && mkdirp lib",
    "build": "rollup -c",
    "preexample": "npm run build",
    "example": "rollup -c examples/rollup.config.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "keywords": [
    "Math",
    "mathematics",
    "vector",
    "underscore.math",
    "helper",
    "util"
  ],
  "author": "Matt Styles",
  "repository": "git://github.com/mattstyles/mathutil.git",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babelrc-rollup": "^3.0.0",
    "buba": "^4.0.1",
    "mkdirp": "^0.5.1",
    "quay": "^0.5.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.40.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-watch": "^3.1.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  }
}
