{
  "name": "mathutil",
  "version": "0.2.0",
  "description": "Various and random collection of maths utilities",
  "main": "lib/index.js",
  "scripts": {
    "test": "tape -r babel-register spec/*.js",
    "prepublish": "npm run build",
    "prepack": "npm run build",
    "watch": "babel src --out-dir lib --watch",
    "prebuild": "rimraf lib && mkdirp lib",
    "build": "babel src --out-dir lib",
    "preexample": "npm run build",
    "example": "budo examples/turn.js:bundle.js -v --dir examples --live"
  },
  "babel": {
    "presets": [
      "urban"
    ]
  },
  "browserify": {
    "transform": "babelify"
  },
  "keywords": [
    "Math",
    "mathematics",
    "vector",
    "underscore.math",
    "helper",
    "util"
  ],
  "author": "Matt Styles",
  "repository": "git://github.com/mattstyles/mathutil.git",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-urban": "^1.0.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^9.4.1",
    "microtime": "^2.1.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "standard": "^8.6.0",
    "tape": "^4.6.3"
  }
}
