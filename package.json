{
  "name": "mathutil",
  "version": "2.4.0",
  "description": "Various and random collection of maths utilities",
  "main": "lib/mathutil.js",
  "module": "lib/mathutil.mjs",
  "jsnext:main": "lib/mathutil.mjs",
  "unpkg": "dist/mathutil.min.js",
  "jsdelivr": "dist/mathutil.min.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf dist lib",
    "test": "tape -r buba/register spec/*.js",
    "prepublish": "npm run build",
    "prepack": "npm run build",
    "watch": "rollup -c -w",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "size": "size-limit",
    "preexample": "npm run build",
    "example": "rollup -c examples/rollup.config.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "lint-staged": {
    "*.js": [
      "standard"
    ],
    "*.jsx": [
      "standard"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test && npm run size"
    }
  },
  "size-limit": [
    {
      "path": "lib/mathutil.js",
      "limit": "4 KB"
    }
  ],
  "keywords": [
    "Math",
    "mathematics",
    "vector",
    "underscore.math",
    "helper",
    "util"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mattstyles/mathutil.git"
  },
  "bugs": {
    "url": "https://github.com/mattstyles/mathutil/issues"
  },
  "homepage": "https://github.com/mattstyles/mathutil#readme",
  "dependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.1.6",
    "babel-eslint": "^10.0.3",
    "buba": "^4.0.2",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "quay": "^0.5.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.23.1",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "size-limit": "^2.1.6",
    "standard": "^14.3.1",
    "tape": "^4.11.0"
  }
}
