{
  "name": "mathutil",
  "version": "3.1.0-beta.0",
  "description": "Various and random collection of maths utilities",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "unpkg": "dist/mathutil.min.js",
  "jsdelivr": "dist/mathutil.min.js",
  "exports": {
    ".": "lib/index.js"
  },
  "files": [
    "build",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "NODE_ENV=production rimraf lib && swc src --config-file .swcrc -d lib",
    "build:esm": "NODE_ENV=production rimraf esm && swc src --config-file .swcrc.esm -d esm",
    "build:watch": "swc -w src -d lib",
    "bundle": "NODE_ENV=production rimraf build && npx spack",
    "types": "NODE_ENV=production tsc --build tsconfig.build.json",
    "types:watch": "tsc -w --build tsconfig.build.json",
    "size": "size-limit",
    "watch": "concurrently --kill-others \"pnpm run build:watch\" \"pnpm run types:watch\" \"pnpm run test:watch\"",
    "example": "pnpm run build && speedrun examples/index.html"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run size"
    }
  },
  "size-limit": [
    {
      "path": "lib/mathutil.js",
      "limit": "4 KB"
    }
  ],
  "sideEffects": false,
  "keywords": [
    "Math",
    "mathematics",
    "vector",
    "underscore.math",
    "helper",
    "util"
  ],
  "author": "Matt Styles",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mattstyles/mathutil.git"
  },
  "bugs": {
    "url": "https://github.com/mattstyles/mathutil/issues"
  },
  "homepage": "https://github.com/mattstyles/mathutil#readme",
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.121",
    "@swc/jest": "^0.2.15",
    "@types/jest": "^27.0.3",
    "chokidar": "^3.5.2",
    "concurrently": "^6.5.1",
    "jest": "^27.4.5",
    "rimraf": "^3.0.2",
    "speedrun": "^1.0.2",
    "typescript": "^4.5.4"
  }
}
